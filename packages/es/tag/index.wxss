.wux-tag {
  color: rgba(0,0,0,.65);
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  list-style: none;
  display: -ms-inline-flexbox;
  display: inline-flex;
  -ms-flex-align: center;
  align-items: center;
  line-height: 40rpx;
  height: 44rpx;
  padding: 0 14rpx;
  border-radius: 8rpx;
  border: 1PX solid #e8e8e8;
  background: #fafafa;
  font-size: 24rpx;
  transition: all .3s cubic-bezier(.215,.61,.355,1);
  opacity: 1;
  margin-right: 16rpx;
  cursor: pointer;
  white-space: nowrap
}
.wux-tag--hover {
  opacity: .85
}

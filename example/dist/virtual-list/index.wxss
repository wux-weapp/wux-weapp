.wux-virtual-list {
  position: relative;
  display: block;
  overflow: auto;
  height: auto
}
.wux-virtual-list__mask {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 2;
  width: 100%;
  height: 100%;
  background-color: transparent
}
.wux-virtual-list__scroll-view {
  width: 100%;
  height: 100%
}
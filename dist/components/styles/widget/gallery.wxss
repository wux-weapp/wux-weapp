.wux-gallery {
  display: block;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: #000000;
  z-index: 1000;
}
.wux-gallery__swiper {
  width: 100%;
  height: 100%;
}
.wux-gallery__item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.wux-gallery__img {
  -webkit-box-flex: 0;
  -webkit-flex: none;
          flex: none;
  width: 100%;
  height: 100%;
}
.wux-gallery__opr {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: #0D0D0D;
  color: #FFFFFF;
  line-height: 120rpx;
  text-align: center;
}
.wux-gallery__del {
  display: block;
}
.wux-gallery__icon {
  background-image: url(data:image/png;base64,);
  background-position: center center;
  background-repeat: no-repeat;
  background-size: contain;
  color: #FFFFFF;
  width: 48rpx;
  height: 48rpx;
  margin: 36rpx auto;
}
